start()
{
#set -x
##-----  Create baseline entry

$RAS_SNAP start -s $S_ID -b $SLOCAL >> $LOG #2>&1;
$RAS_SNAP fail -s $S_ID -b $SLOCAL >>  $LOG #2>&1;

} #----------------------------  START  -------------------------------

